#simulate data from various negative binomial distributions
B <- 1000
plist <- c(.1,.5,.9)
mu <- 50
numDatasets <- 5
mat <- matrix(0, nrow=B+2,ncol=length(plist)*numDatasets)
count <- 1
n <- 1000
for(i in 1:length(plist)){
for(j in 1:numDatasets){
set.seed(j)
p <- plist[i]
r <- mu*p/(1-p)
x <- rnbinom(B, r,p)
print(var(x)/mean(x))
mat[,count] <- c(p, j, x)
count <- count + 1
}
}
write.csv(mat, "NBdata.csv")
